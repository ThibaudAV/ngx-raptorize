{
  "name": "ngx-raptorize",
  "version": "0.0.0-semantically-released",
  "description": "An awesome ngx library that unleashes a Raptor of Jurassic proportions... Well, technically it's Cretaceous proportions, but we'll let that slide for now.",
  "repository": "https://github.com/ThibaudAV/ngx-raptorize",
  "author": "ThibaudAv",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build -c production",
    "build:ngx-raptorize": "ng build ngx-raptorize && cp -R projects/ngx-raptorize/src/assets/ dist/ngx-raptorize/assets/",
    "test": "ng test",
    "lint": "yarn lint:commit && yarn ng lint && yarn lint:prettier",
    "lint:commit": "commitlint --from=$( git rev-list --tags --max-count=1 )",
    "lint:prettier": "yarn prettier -l '**/*.{ts,js,scss,md,json}'",
    "lint:prettier:fix": "yarn prettier --write '**/*.{ts,js,scss,md,json}'",
    "semantic-release": "semantic-release"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "release": {
    "pkgRoot": "dist/ngx-raptorize"
  },
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/router": "~8.2.11",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.10",
    "@angular-devkit/build-ng-packagr": "~0.803.10",
    "@angular/cli": "~8.3.10",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "husky": "^3.0.9",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "semantic-release": "^15.13.27",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
