{
  "name": "ngx-raptorize",
  "version": "0.0.0-semantically-released",
  "description": "An awesome ngx library that unleashes a Raptor of Jurassic proportions... Well, technically it's Cretaceous proportions, but we'll let that slide for now.",
  "repository": "https://github.com/ThibaudAV/ngx-raptorize",
  "author": "ThibaudAv",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build -c production",
    "build:ngx-raptorize": "ng build ngx-raptorize && cp -R projects/ngx-raptorize/src/assets/ dist/ngx-raptorize/assets/",
    "test": "ng test",
    "lint": "yarn lint:commit && yarn ng lint && yarn lint:prettier",
    "lint:commit": "commitlint --from=$( git rev-list --tags --max-count=1 )",
    "lint:prettier": "yarn prettier -l '**/*.{ts,js,scss,md,json}'",
    "lint:prettier:fix": "yarn prettier --write '**/*.{ts,js,scss,md,json}'",
    "semantic-release": "semantic-release"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "release": {
    "pkgRoot": "dist/ngx-raptorize"
  },
  "dependencies": {
    "@angular/animations": "~9.0.4",
    "@angular/common": "~9.0.4",
    "@angular/compiler": "~9.0.4",
    "@angular/core": "~9.0.4",
    "@angular/forms": "~9.0.4",
    "@angular/platform-browser": "~9.0.4",
    "@angular/platform-browser-dynamic": "~9.0.4",
    "@angular/router": "~9.0.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular-devkit/build-ng-packagr": "~0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.4",
    "@angular/language-service": "~9.0.4",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jasmine": "~3.5.7",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^13.7.7",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.0",
    "prettier": "^1.19.1",
    "protractor": "~5.4.3",
    "semantic-release": "^17.0.4",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5"
  }
}
